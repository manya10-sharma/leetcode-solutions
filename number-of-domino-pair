class Solution {
    public int numEquivDominoPairs(int[][] dominoes) {
        int[] freq = new int[100]; 
        int count = 0;
        for (int[] d : dominoes) {
            int a = d[0];
            int b = d[1];
            int x = Math.min(a, b);
            int y = Math.max(a, b);
            int key = x * 10 + y;
            count += freq[key];
            freq[key]++;
        }
        return count;
    }
}